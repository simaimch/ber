{
    "subway":{
        "listen": "timePass",
        "arguments":"subway",
        "actions":{
            "interrupt":"subway.event"
        }
    },
    "hourUpdate":{
        "listen":"timePass_HOUR",
        "actions":{
            "PlayerData":{
                "money":{"mode":"inc","value":10000}
            }
        }
    },
    "dayUpdate":{
        "listen":"timePass_DAY",
        "actions":{}
    },
    "hairGrowth":{
        "listen":"timePass_DAY",
        "actions":{
            "foreach":{
                "iterate": "PlayerData.body.hair",
                "PARAM1":{
                    "length":{"mode":"inc","#value":"PARAM1.growth"}
                },
                "modifierUpdate":{"keyword":"hair"}
            }
        }
    },
    "levelup_makeup":{
        "listen": "levelup",
        "arguments":"makeup",
        "actions":{
            "message":{
                "^text":"Makeup: Your skill increased to #PlayerData.skill.makeup.level#!"
            }
        }
    },
    "onOutfitUpdate":{
        "listen": "outfitUpdate",
        "arguments":"*",
        "actions":{
            "PlayerData":{
                "outfit.CURRENT_TYPE":{"mode":"set","#value":"outfitType(PlayerData.outfit.CURRENT)"}
            },
            "modifierUpdate":{"keyword":"outfit"}
        }
    },
    "shower":{
        "listen": "timePass",
        "arguments":"shower",
        "actions":{
            "interrupt":"bath.shower"
        }
    },
    "sleepEndEarly":{
        "listen": "sleepEnd",
        "arguments":"early",
        "actions":{"interrupt":"sleeping.wakeEarly"}
    },
    "sleepEndHealthy":{
        "listen": "sleepEnd",
        "arguments":"healthy",
        "actions":{"interrupt":"sleeping.wakeHealthy"}
    }
    ,
    "sleepEndHungry":{
        "listen": "sleepEnd",
        "arguments":"hunger",
        "actions":{"interrupt":"sleeping.wakeHungry"}
    }
    ,
    "sleepEndThirsty":{
        "listen": "sleepEnd",
        "arguments":"thirst",
        "actions":{"interrupt":"sleeping.wakeThirsty"}
    }
}
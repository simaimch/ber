{
    "subway":{
        "listen": "timePass",
        "arguments":"subway",
        "actions":{
            "interrupt":"subway.event"
        }
    },
    "testEvent":{
        "listen": "timePass",
        "arguments":"*",
        "mtth": 5,
        "actions":{}
    },
    "hourUpdate":{
        "listen":"timePass_HOUR",
        "actions":{
            "PlayerData":{
                "money":{"mode":"inc","value":10000}
            }
        }
    },
    "dayUpdate":{
        "listen":"timePass_DAY",
        "actions":{}
    },
    "hairGrowth":{
        "listen":"timePass_DAY",
        "actions":{
            "PlayerData":{
                "body.hair.length":{"mode":"inc","value":500}
            }
        }
    },
    "sleepEndEarly":{
        "listen": "sleepEnd",
        "arguments":"early",
        "actions":{"interrupt":"sleeping.wakeEarly"}
    },
    "sleepEndHealthy":{
        "listen": "sleepEnd",
        "arguments":"healthy",
        "actions":{"interrupt":"sleeping.wakeHealthy"}
    }
    ,
    "sleepEndHungry":{
        "listen": "sleepEnd",
        "arguments":"hunger",
        "actions":{"interrupt":"sleeping.wakeHungry"}
    }
    ,
    "sleepEndThirsty":{
        "listen": "sleepEnd",
        "arguments":"thirst",
        "actions":{"interrupt":"sleeping.wakeThirsty"}
    }
}
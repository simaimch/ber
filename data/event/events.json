{
    "subway":{
        "listen": "timePass",
        "arguments":"subway",
        "actions":{
            "interrupt":"subway.event"
        }
    },
    "walk":{
        "listen":"timePass",
        "arguments":"walk",
        "~actions":{
            "0":{
                "condition":"items[PlayerData.outfit.CURRENT.shoes].height > 0",
                "value":{
                    "PlayerData":{
                        "body.feet.pain.factors.heels.current":{"mode":"inc","&value":"shoePainPC(items[PlayerData.outfit.CURRENT.shoes],PlayerData)*PARAM1"}
                    },
                    "skillCheck":{
                        "skill":"heels",
                        "#difficulty":"items[PlayerData.outfit.CURRENT.shoes].difficulty",
                        "&experience":"PARAM1*2",
                        "result":{
                            "0":{
                                "modifierUpdate":{"keyword":"makeup"}
                            }
                        }
                    }
                    
                }
            }
        }
    },
    "hourUpdate":{
        "listen":"timePass_HOUR",
        "actions":{
            "PlayerData":{
                "money":{"mode":"inc","value":10000}
            }
        }
    },
    "dayUpdate":{
        "listen":"timePass_DAY",
        "actions":{}
    },
    "hairGrowth":{
        "listen":"timePass_DAY",
        "actions":{
            "foreach":{
                "iterate": "PlayerData.body.hair",
                "PARAM1":{
                    "length":{"mode":"inc","#value":"PARAM1.growth"}
                },
                "modifierUpdate":{"keyword":"hair"}
            }
        }
    },
    "levelup_heels":{
        "listen": "levelup",
        "arguments":"heels",
        "actions":{
            "message":{
                "^text":"High Heels: Your skill increased to #PlayerData.skill.heels.level#!"
            },
            "modifierUpdate":{"keyword":"skillHeels"}
        }
    },
    "levelup_makeup":{
        "listen": "levelup",
        "arguments":"makeup",
        "actions":{
            "message":{
                "^text":"Makeup: Your skill increased to #PlayerData.skill.makeup.level#!"
            }
        }
    },
    "onOutfitUpdate":{
        "listen": "outfitUpdate",
        "arguments":"*",
        "actions":{
            "PlayerData":{
                "outfit.CURRENT_TYPE":{"mode":"set","#value":"outfitType(PlayerData.outfit.CURRENT)"}
            },
            "modifierUpdate":{"keyword":"outfit"}
        }
    },
    "shower":{
        "listen": "timePass",
        "arguments":"shower",
        "actions":{
            "interrupt":"bath.shower"
        }
    },
    "sleepEndEarly":{
        "listen": "sleepEnd",
        "arguments":"early",
        "actions":{"interrupt":"sleeping.wakeEarly"}
    },
    "sleepEndHealthy":{
        "listen": "sleepEnd",
        "arguments":"healthy",
        "actions":{"interrupt":"sleeping.wakeHealthy"}
    }
    ,
    "sleepEndHungry":{
        "listen": "sleepEnd",
        "arguments":"hunger",
        "actions":{"interrupt":"sleeping.wakeHungry"}
    }
    ,
    "sleepEndThirsty":{
        "listen": "sleepEnd",
        "arguments":"thirst",
        "actions":{"interrupt":"sleeping.wakeThirsty"}
    },
    "stateHungerUpdate":{
        "listen":"timePass_10Minutes",
        "actions":{"modifierUpdate":{"keyword":"hunger"}}
    },
    "stateThirstUpdate":{
        "listen":"timePass_10Minutes",
        "actions":{"modifierUpdate":{"keyword":"thirst"}}
    }
}